file = {:Users/davidcoeurjolly/Mendeley Desktop/Rong et al./IEEE transactions on visualization and computer graphics/Rong et al. - 2011 - GPU-Assisted Computation of Centroidal Voronoi Tessellation.pdf:pdf},
